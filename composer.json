{
  "name": "oparl/spec-website",
  "description": "Die OParl-Spezifikation online ansehen oder herunterladen.",
  "keywords": [
    "spec",
    "oparl",
    "download",
    "view"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.4.*",
    "graham-campbell/github": "^5.1",
    "predis/predis": "^1.1",
    "cocur/slugify": "^2.5",
    "efrane/letterpress": "dev-master",
    "efrane/laravel-deploy": "dev-master",
    "efrane/transfugio": "dev-master",
    "fzaninotto/faker": "^1.6",
    "barryvdh/laravel-debugbar": "^2.3",
    "oneblackbear/roman-numbers": "@dev",
    "symfony/dom-crawler": "^3.2",
    "symfony/css-selector": "^3.2",
    "composer/semver": "^1.4",
    "piwik/piwik-php-tracker": "^1.2",
    "doctrine/dbal": "^2.5",
    "laravel/tinker": "^1.0",
    "guzzlehttp/guzzle": "^6.2",
    "php-http/guzzle6-adapter": "^1.1",
    "dompdf/dompdf": "^0.8.0",
    "zircote/swagger-php": "^2.0",
    "morrislaptop/laravel-queue-clear": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "phpspec/phpspec": "~3.2",
    "mockery/mockery": "^0.9",
    "phpunit/phpunit-selenium": "^3.0",
    "mikey179/vfsStream": "^1.6",
    "laravel/browser-kit-testing": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "OParl\\Spec\\": "lib/Spec/",
      "OParl\\Server\\": "lib/Server",
      "OParl\\Website\\API\\": "lib/Api/",
      "EFrane\\Akismet\\": "lib/Akismet/",
      "EFrane\\Buildkite\\": "lib/Buildkite/",
      "EFrane\\HubSync\\": "lib/HubSync/src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
