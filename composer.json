{
  "name": "oparl/spec-website",
  "description": "Die OParl-Spezifikation online ansehen oder herunterladen.",
  "keywords": [
    "spec",
    "oparl",
    "download",
    "view"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.1.*",
    "graham-campbell/github": "^3.2",
    "predis/predis": "^1.0",
    "cocur/slugify": "^1.2",
    "pda/pheanstalk": "^3.1",
    "efrane/letterpress": ">=0.9",
    "efrane/laravel-deploy": "dev-master",
    "efrane/transfugio": "dev-master",
    "fzaninotto/faker": "^1.5",
    "barryvdh/laravel-debugbar": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "OParl\\Spec\\": "lib/Spec/",
      "OParl\\Server\\": "lib/Server",
      "EFrane\\Akismet\\": "lib/Akismet/",
      "EFrane\\Buildkite\\": "lib/Buildkite/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
